{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/*"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }    
  ],
  "dataGroups": [
    {
      "name": "app",
      "urls": [
        "/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 1000,
        "maxAge": "3d"
      }
    },
    {
      "name": "firebase",
      "urls": ["https://epi-serv-job-default-rtdb.firebaseio.com/**"],
        "cacheConfig": {
          "strategy": "performance",
          "maxSize": 1000,
          "maxAge": "3d"
        }
    }    
  ]
}
